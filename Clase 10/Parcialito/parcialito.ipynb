    "def normal_pdf(x, mu, sigma):\n",
    "    fraction = 1/(sigma*np.sqrt(2*np.pi))\n",
    "    exp_term = -.5 * ((x-mu)/sigma)**2\n",
    "    return fraction * np.exp(exp_term)"
